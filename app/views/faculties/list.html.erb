<% if $faculty.present? %>
<div id="fixfooter">
<br>
<h1 align="center">Students-List</h1>
<br>
<table class="table table-striped table-bordered table-hover">
  <thead class="thead-dark">
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Father name</th>
      <th>Roll no</th>
      <th>Email</th>
      <th>Student class</th>
      <th>Mobile</th>
      <th>Action</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @sdetail.each do |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.last_name %></td>
        <td><%= student.father_name %></td>
        <td><%= student.roll_no %></td>
        <td><%= student.email_id %></td>
        <td><%= student.student_class %></td>
        <td><%= student.mobile %></td>
        <td><%= link_to 'Insert-Marks', student %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br><br><br><br>
<%= link_to 'Back to Dashboard',faculty_dashboard_path %>
<br><br>
</div>

<% else %>
<div id="homepage">
      <br> <br><br><br>
      <h1 id="heading">Welcome in ERP portal</h1>
      <br><br>
      <h1 style="margin-left:130px;color:red;">Error,404</h1>
      <h2 style="margin-left:130px;color:red;">Page Not Found</h2>
</div>
<% end %>